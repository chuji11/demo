html,body,header,nav,article,div,span,input,ul,li,a,img,p,section,footer{
   margin: 0;padding: 0;
   /*box-sizing: border-box;*/
}
ul li{
   list-style: none;
}
a{
   text-decoration: none;
   color:#999;
}
.header{
   width: 100%;
   height:98px;
   
}
.header-box{
   width:1130px;
   margin: 0 auto;
}
.logo{
   display: block;
   height: 98px;
   width: 200px;
   
}
.logo>img{
   padding-top:25px;
}
.logo>div>p {
   font-size: 30px;
   color:#000;

}
.logo>div>span{
   font-size: 10px;
   color:#333;

}

.logo>div{
   display: inline-block;
   padding-left:15px;
}
.big-box{
   height: 524px;
}
.big-bg{
   width: 100%;
   height: 588px;
   position: absolute;
   left: 0;
   top: 98px;
   background-image: url(../images/mi9.jpg);
   background-repeat: no-repeat;
   background-position: top center;
   
}
.big-bg .big-bg-a{
   width:100%;
   height:100%;
   display: block;
   position: absolute;
}
.big-bg .login-box{
   width:1130px;
   margin: 0 auto;

}
.big-bg .login{
   width: 410px;
   min-height: 524px;
   margin-bottom: 0;
   margin-right: 0;
   font-size: 14px;
   z-index: 4;
   background-color: #fff;
   text-align: center;
   margin-top: 32px;
   position: relative;
   left:100%;
   margin-left:-410px;
}
.login .login-nav{
   padding: 27px 0 24px;
   color:#666;
}
.login-left.color,.login-right.color{
   color: #ff6700;
}
.login-left,.login-right{
   font-size: 24px;
   display: inline-block;
   width: 201px;
   color: #666;
}
.login-nav a:hover{
   color: #ff6700;
}
.login-right{
   border-left:solid 2px #e0e0e0;
}
.login .login-form{
   width:348px;
   padding: 0 31px;
   margin-top:10px;
}
.login .login-form div input{
   width: 348px;height:50px;
   margin-bottom: 14px;
   border: 1px solid #e0e0e0;
   line-height: 22px;
   padding: 13px 16px 13px 14px;
   box-sizing: border-box;
   outline: none;
}
.login-form .phone-bef{
   width:66px;
   height:50px;
   display: block;
   border: 1px solid #e0e0e0;
   border-right:none;
   margin-bottom: 14px;
   float: left;
   box-sizing: border-box;
}
.login-form .phone-bef span{
   display: inline-block;
   line-height: 50px;
}
.login .login-form #phone-login .inp1{
   width:282px;
   display: inline-block;
}

.login .login-form #phone-login .inp2{
   width: 236px;
}
.login-form .xin-bef{
   width:112px;
   height:50px;
   display: block;
   border: 1px solid #e0e0e0;
   border-left:none;
   margin-bottom: 14px;
   float: right;
   box-sizing: border-box;
}
.login-form .xin-bef a{
   color:#003ab5;
   line-height: 50px;
}
.login-form .un-box{
   height:30px;line-height: 30px;
   color:#f66;
   text-align: left;
}
.login-form .un-box .icon-error{
   background: url(../images/icon_user.png) no-repeat;
   background-position: left -53px top 4px;
   line-height: 30px;
   height: 30px;
   width: 17px;
   display: inline-block;
   background-size: 70px;
   vertical-align: middle;
}
.un-box>span{
   padding-bottom: 5px;
   display: inline-block;
   vertical-align: middle;
}
#btn input{
   width:348px;height:50px;
   margin-top:10px;
   background-color: #ff6700;
   font-size: 14px;
   color: #fff;
   cursor: pointer;
}
.login-area{
   color:#999;
}

.login-area .color{
   color:#ff6700
}
.login-area a:hover{
   color:#ff6700;
}
fieldset {
   display: inline-block;
   width: 327px;
   margin-top: 150px;
   color: #999;
   border: none;
   border-top: 1px solid #e0e0e0;
}
.icon a{
   font-size: 30px;
   margin: 0 17px;
}
.icon>a:first-child{
   color: #0288d1;
}
.icon a:nth-child(2){
   color:#d32f2f;
}
.icon a:nth-child(3){
   color:#0ae;
}
.icon a:last-child{
   color:#00d20d;
}
.login-sm{
   height:442px;
}
.login-sm-box{
   padding-top:105px;
}

.login-sm .cm-txt{
   padding-top:14px;
   color:#757575;
}
.login-sm .cm-txt span{
   color:#ff6700;
   padding: 0 5px;

}
.write{
   height: 64px;
   width:100%;
   position: relative;
   background-color:#fff;
   top:524px;
}
.footer{
   width:100%;
   height: 80px;
   padding-top: 100px;
   position: relative;
   font-size: 14px;
}

.foot-font{
   text-align: center;
}
.foot-font ul li{
   display: inline-block;
   padding-top:10px;
}
.foot-font ul li a{
   padding: 0 10px;
   color:#757575;
}
.foot-font ul li a:hover{
   
   color:#333;
}
.foot-copy{
   padding:10px 0;
   text-align: center;
   color: #9d9d9d;
}
.foot-copy a{
   color:#9d9d9d;
}