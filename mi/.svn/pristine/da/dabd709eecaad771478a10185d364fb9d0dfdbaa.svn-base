html,
body,
header,
nav,
article,
div,
span,
input,
ul,
li,
a,
img,
p,
section,
footer {
  margin: 0;
  padding: 0;
  /* display: block; */
  box-sizing: border-box;
}
body {
  font-size: 14px;
  background: #f9f9f9;
}
ul li {
  list-style: none;
}
a {
  text-decoration: none;
}
.wrap {
  width: 100%;
  min-height: 100%;
  height: auto;
}
.box {
  background: #fff;
  width: 854px;
  height: 559px;
  margin: 0 auto;
}
.box .main {
  padding: 0px 34px 0;
}
.milogo {
  background: url(../images/milogo.png) no-repeat;
  height: 48px;
  width: 49px;
  margin: 0 auto;
  padding-bottom: 94px;
  display: block;
}
.main .title {
  font-size: 30px;
  font-weight: 400;
  text-align: center;
}
.main .regbox {
  margin: 0 auto;
  width: 332px;
  height: 306px;
  padding: 30px 0;
}
.def-country {
  height: 40px;
  padding-left: 14px;
  line-height: 40px;
  display: inline-block;
  vertical-align: middle;
  border: 1px solid #e8e8e8;
  color: #555;
  cursor: pointer;
  width: 332px;
}
.regbox p {
  padding-bottom: 5px;
}
.def-country > div {
  display: inline-block;
  border-right: solid 1px #e8e8e8;
  width: 276px;
}
.icon {
  float: right;
  background: url(../images/icon_user.png);
  width: 16px;
  height: 16px;
  margin: 11px 12px 0 12px;
}
.text {
  margin-bottom: 15px;
  padding-top: 5px;
}
.pad-box {
  padding-bottom: 15px;
}
.result-box {
  border: 1px solid #e8e8e8;
}
.result-id {
  width: 74px;
  height: 42px;
  display: block;
  border-right: 1px solid #e8e8e8;
  float: left;
  box-sizing: border-box;
  text-align: center;
  cursor: pointer;
  color: #555;
  font-size: 12px;
}
.result-id > span {
  display: inline-block;
  line-height: 40px;
}
.result-id > i {
  width: 20px;
  font-size: 16px;
  display: inline-block;
  line-height: 40px;
}
.phone {
  width: 256px;
  height: 40px;
  display: inline-block;
  border: none;
  padding-left: 10px;
  outline: none;
}
.un-box {
  height: 30px;
  line-height: 30px;
  color: #f66;
}
.un-box .icon-error {
  background: url(../images/icon_user.png) no-repeat;
  background-position: left -53px top 4px;
  line-height: 30px;
  height: 30px;
  width: 17px;
  display: inline-block;
  background-size: 70px;
  vertical-align: middle;
}
.un-box > span {
  padding-bottom: 5px;
  display: inline-block;
  vertical-align: middle;
}
#btn {
  background-color: #ff6700;
  margin-top: 15px;
  border: 1px solid rgba(186, 186, 186, 0.3);
  width: 332px;
  height: 42px;
  color: #fff;
  cursor: pointer;
  outline: none;
}
#btn:hover {
  opacity: 0.7;
}
.privay-box {
  padding-top: 40px;
  text-align: center;
  color: #666;
}
.privay {
  padding: 10px 0;
  cursor: pointer;
  margin: 0 auto;
  display: inline-block;
}
.treaty {
  display: inline-block;
}
.privay i {
  background: url(../images/icon_user.png) no-repeat;
  display: inline-block;
  width: 18px;
  height: 18px;
  background-position: left -37px top 0px;
  background-size: 70px;
  vertical-align: middle;
}

.privay > a {
  color: #333;
  font-weight: bold;
}
.footer {
  width: 100%;
  height: 80px;
}
.foot-font {
  text-align: center;
}
.foot-font ul li {
  display: inline-block;
  padding-top: 10px;
}
.foot-font ul li a {
  padding: 0 10px;
  color: #757575;
}
.foot-font ul li a:hover {
  color: #333;
}
.foot-copy {
  padding: 10px 0;
  text-align: center;
  color: #9d9d9d;
}
.foot-copy a {
  color: #9d9d9d;
}

.dis-country,
.phone-box {
  border: solid 1px #e8e8e8;
  border-top: none;
  width: 332px;
  height: 270px;
  overflow: auto;
  position: absolute;
  background: #fff;
}
.dis-country .seach,
.phone-box .seach {
  margin-left: 14px;
  width: 284px;
  height: 34px;
  padding: 5px 10px 5px 10px;
  border: 1px solid #e0e0e0;
  margin: 10px 14px;
}
.dis-country ul > li,
.phone-box ul li {
  width: 313px;
  border-bottom: solid 1px #e8e8e8;
  background: #fff;
  height: 40px;
  padding: 0 14px;
  line-height: 40px;
}
.dis-country ul > li > span,
.phone-box ul > li > span {
  float: right;
}
.dis-country ul > li.bg-color,
.phone-box ul > li.bg-color {
  background: #999;
  opacity: 0.7;
}
