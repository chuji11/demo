html,body,h1,h2,h3,h4,h5,h6,a,p,img,input,ul,li,ol,dl,dt,dd,form,table,tr,td,b,i,div,span,hr{
    margin:0;
    padding:0;
    font-size:12px;
}
a{
    text-decoration: none;
    color: #424242;
}
ul li,ol li{
    list-style-type: none;
}
a:link{
    text-decoration: none;
}
img{
    border:none;
}
h1,h2,h3,h4,h5,h6,b,strong{
    font-weight: normal;
}
.clearfix::after{
    visibility: hidden;
    content:".";
    clear:both;
    font-size:0px;
    overflow: hidden;
    display: block;
    height:0;
}